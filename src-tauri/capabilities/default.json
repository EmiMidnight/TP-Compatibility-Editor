{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:allow-write-text-file",
    "core:path:default",
    {
      "identifier": "fs:scope-data",
      "allow": [
        {
          "path": "*/**"
        }
      ],
      "deny": []
    },
    "dialog:default"
  ]
}